<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4"
      main_tree_to_execute="red_coba_nasional">
  <BehaviorTree ID="blue_silo2">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="9.5;2.0;-1.57 /&#10;8.73;4.8;-1.57"/>
      <AMCLUpdate/>
      <NavToPose frame_id="map"
                 goal="8.73;4.8;-1.57"/>
      <SetSpeed x="-1.5"
                y="0.0"
                theta="0.0"
                timeout="1000"/>
      <MoveWithLidarReference parent_frame="map"
                              child_frame="base_link"
                              x="0.0"
                              y="2.78"
                              speed="1.5"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="findnewballtest">
    <RetryUntilSuccessful num_attempts="-1">
      <Sequence>
        <SetIntakeMechanism dribble="3"
                            lift="0.3"/>
        <Sequence>
          <ReactiveFallback>
            <BallGrabbed color="blue"/>
            <GrabNearestBall color="blueball"
                             id="{id}"/>
            <Repeat num_cycles="3">
              <Sequence>
                <NavToPose frame_id="map"
                           goal="9.5;2.0;-1.0"/>
                <Sleep msec="2000"/>
                <NavToPose frame_id="map"
                           goal="9.5;2.0;-2.07"/>
              </Sequence>
            </Repeat>
          </ReactiveFallback>
          <ReactiveFallback>
            <BallGrabbed color="blue"/>
            <BallGrabbed color="purple"/>
            <Follow color="blueball"
                    id="{id}"/>
          </ReactiveFallback>
          <FollowPose frame_id="base_link"
                      goal="0;0;0"/>
        </Sequence>
        <Fallback>
          <BallGrabbed color="blue"/>
          <ForceFailure>
            <Sequence>
              <Rotate pose=""
                      angle="-2.5"
                      rotated_pose=""
                      frame_id=""/>
              <Sleep msec="1000"/>
              <SetIntakeMechanism dribble="0"
                                  lift="2"/>
              <GetIntakeProximityArray pattern="000"/>
              <Rotate pose=""
                      angle="-1.57"
                      rotated_pose=""
                      frame_id=""/>
              <Sleep msec="1000"/>
              <SetIntakeMechanism dribble="3"
                                  lift="0.5"/>
            </Sequence>
          </ForceFailure>
        </Fallback>
        <GetIntakeProximityArray pattern="010"/>
        <SetIntakeMechanism dribble="0"
                            lift="0"/>
      </Sequence>
    </RetryUntilSuccessful>
  </BehaviorTree>

  <BehaviorTree ID="grabball">
    <ReactiveFallback>
      <Fallback>
        <BallGrabbed color="blue"/>
        <BallGrabbed color="purple"/>
        <GrabNearestBall color="blueball"
                         id="{id}"/>
      </Fallback>
      <Repeat num_cycles="-1">
        <Sequence>
          <NavToPose frame_id="map"
                     goal="9.5;2.0;-0.8"/>
          <Sleep msec="3000"/>
          <NavToPose frame_id="map"
                     goal="9.5;2.0;-2.3"/>
        </Sequence>
      </Repeat>
    </ReactiveFallback>
  </BehaviorTree>

  <BehaviorTree ID="intakenew">
    <Sequence>
      <SetIntakeMechanism dribble="3"
                          lift="0.8"/>
      <GetBallGrabbed color_output=""
                      color_input="blue"/>
      <SetIntakeMechanism dribble="0"
                          lift="0.5"/>
      <FollowPose frame_id="base_link"
                  goal="0;0;0"/>
      <GetBallGrabbedTop color="blue"/>
      <SetIntakeMechanism dribble="0"
                          lift="0"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="maintree">
    <SequenceWithMemory>
      <SubTree ID="movetest"/>
      <Repeat num_cycles="9">
        <Sequence>
          <SubTree ID="findandfollowball"
                   _autoremap="true"/>
          <Sleep msec="1000"/>
          <SubTree ID="blue_silo2"/>
          <SubTree ID="outtake"
                   _autoremap="false"/>
          <NavThroughPoses frame_id="map"
                           goals="9.5;0.8;-1.57"/>
          <SubTree ID="findandfollowball"
                   _autoremap="true"/>
          <Sleep msec="1000"/>
          <SubTree ID="silo3"/>
          <SubTree ID="outtake"
                   _autoremap="false"/>
          <NavThroughPoses frame_id="map"
                           goals="9.5;0.8;-1.57"/>
          <SubTree ID="findandfollowball"/>
          <Sleep msec="1000"/>
          <SubTree ID="silo4"/>
          <SubTree ID="outtake"/>
          <NavThroughPoses frame_id="map"
                           goals="9.5;0.8;-1.57"/>
        </Sequence>
      </Repeat>
    </SequenceWithMemory>
  </BehaviorTree>

  <BehaviorTree ID="movelidartest">
    <Repeat num_cycles="5">
      <Sequence>
        <MoveWithLidarReference parent_frame="map"
                                child_frame="base_link"
                                x="0.0"
                                y="2.78"
                                speed="0.5"/>
        <Sleep msec="2000"/>
        <MoveWithLidarReference parent_frame="map"
                                child_frame="base_link"
                                x="0.0"
                                y="2.0"
                                speed="0.5"/>
        <Sleep msec="2000"/>
        <MoveWithLidarReference parent_frame="map"
                                child_frame="base_link"
                                x="0.0"
                                y="1.25"
                                speed="0.5"/>
        <Sleep msec="2000"/>
      </Sequence>
    </Repeat>
  </BehaviorTree>

  <BehaviorTree ID="movemir">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="4.8;0.0;-3.14 /&#10;5.1;-0.1;-3.14 /&#10;5.8;-0.4;-3.14 /&#10;5.9;-0.8;-3.14 /&#10;5.7;-2.0;-3.14 /&#10;5.7;-3.0;-3.14 /&#10;5.9;-3.8;-3.14"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="movemirror">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="4.8;0.0;-3.14 /&#10;5.1;-0.1;-3.14 /&#10;5.8;-0.4;-3.14 /&#10;5.9;-0.8;-3.14 /&#10;5.7;-2.0;-3.14 /&#10;5.7;-3.0;-3.14 /&#10;6.1;-3.75;-3.14 /&#10;8.5;-3.8;-3.14 /&#10;9.5;-3.8;1.57"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="movemovemove">
    <Sequence>
      <WaitButton/>
      <NavThroughPoses frame_id="map"
                       goals="9.5;0.5;-1.57"/>
      <NavThroughPoses frame_id="map"
                       goals="9.5;1.5;-1.57"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="movenew">
    <Sequence>
      <SetupMenu silo2="{silo2}"
                 silo3="{silo3}"
                 silo4="{silo4}"/>
      <WaitButton/>
      <NavThroughPoses frame_id="map"
                       goals="4.8;0.0;-3.14 /&#10;5.2;0.1;-3.14 /&#10;5.6;0.4;-3.14 /&#10;5.9;0.8;-3.14 /&#10;6.2;3.74;-1.57"/>
      <SetIntakeMechanism dribble="0.0"
                          lift="0.3"/>
      <SetTailPosition angle="0.7"/>
      <NavThroughPoses frame_id="map"
                       goals="8.0;3.74;-1.57/&#10;8.4;3.45;-1.57 /&#10;8.5;3.25;-1.57 /&#10;9.0;3.05;-1.57 /&#10;9.5;1.3;-1.57 /"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="movepivot">
    <Sequence>
      <RotateSpeed speed="6.0"
                   angle="-1.57"/>
      <NavThroughPoses frame_id="map"
                       goals="8.70;2.0;-1.57 /&#10;8.70;3.0;-1.57"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="movetest">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="4.8;0.0;-3.14 /&#10;5.2;0.1;-3.14 /&#10;5.6;0.4;-3.14 /&#10;5.8;0.8;-3.14 /&#10;6.0;3.74;-1.57"/>
      <SetIntakeMechanism dribble="-3"
                          lift="0.5"/>
      <SetTailPosition angle="0.7"/>
      <NavThroughPoses frame_id="map"
                       goals="8.0;3.74;-1.57/&#10;8.4;3.45;-1.57 /&#10;8.5;3.25;-1.57 /&#10;8.7;3.05;-1.57 /&#10;9.0;1.0;-1.57 /&#10;9.5;1.5;-1.57"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="newfindandfollow">
    <Sequence>
      <Parallel failure_count="1"
                success_count="-1">
        <SubTree ID="oldfindnewball"/>
        <SubTree ID="intakenew"/>
      </Parallel>
      <NavToPose frame_id="base_link"
                 goal="0;0;0"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="newfindball">
    <RetryUntilSuccessful num_attempts="-1">
      <Sequence>
        <SetIntakeMechanism dribble="3"
                            lift="0.5"/>
        <Fallback>
          <BallGrabbed color="blue"/>
          <ForceFailure>
            <Sequence>
              <BallGrabbed color="purple"/>
              <SubTree ID="rotateToTarget"
                       _autoremap="false"/>
            </Sequence>
          </ForceFailure>
          <Sequence>
            <BallGrabbed color="null"/>
            <SubTree ID="grabball"
                     _autoremap="true"/>
            <ReactiveFallback>
              <BallGrabbed color="blue"/>
              <BallGrabbed color="purple"/>
              <Follow color="blueball"
                      id="{id}"/>
            </ReactiveFallback>
          </Sequence>
        </Fallback>
        <BallGrabbed color="blue"/>
        <GetIntakeProximityArray pattern="110"/>
        <SetIntakeMechanism dribble="1"
                            lift="0"/>
        <NavToPose frame_id="base_link"
                   goal="0;0;0"/>
      </Sequence>
    </RetryUntilSuccessful>
  </BehaviorTree>

  <BehaviorTree ID="newintake">
    <RetryUntilSuccessful num_attempts="-1">
      <Sequence>
        <SetIntakeMechanism dribble="1"
                            lift="0.5"/>
        <Fallback>
          <BallGrabbed color="blue"/>
          <BallGrabbed color="purple"/>
        </Fallback>
        <GetIntakeProximityArray pattern="010"/>
        <SetIntakeMechanism dribble="0"
                            lift="0"/>
      </Sequence>
    </RetryUntilSuccessful>
  </BehaviorTree>

  <BehaviorTree ID="newrotate">
    <Sequence>
      <GetIntakeProximityArray pattern="100"/>
      <SetIntakeMechanism dribble="0"
                          lift="0"/>
      <NavThroughPoses frame_id="map"
                       goals="9.5;0.5;0.0"/>
      <SetIntakeMechanism dribble="0.0"
                          lift="2.0"/>
      <GetIntakeProximityArray pattern="001"/>
      <SetIntakeMechanism dribble="3"
                          lift="1.0"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="oldfindnewball">
    <RetryUntilSuccessful num_attempts="-1">
      <Sequence>
        <RetryUntilSuccessful num_attempts="-1">
          <Sequence>
            <NavToPose frame_id="base_link"
                       goal="0;0;0"/>
            <ReactiveFallback>
              <BallGrabbed color="blue"/>
              <GrabNearestBall color="blueball"
                               id="{id}"/>
              <Repeat num_cycles="-1">
                <Sequence>
                  <NavToPose frame_id="map"
                             goal="9.5;-2.5;1.57"/>
                  <Sleep msec="2000"/>
                  <NavToPose frame_id="map"
                             goal="10.5;-2.5;1.57"/>
                  <Sleep msec="2000"/>
                  <NavToPose frame_id="map"
                             goal="8.5;-2.5;1.57"/>
                  <Sleep msec="2000"/>
                </Sequence>
              </Repeat>
            </ReactiveFallback>
            <NavToPose frame_id="base_link"
                       goal="0;0;0"/>
            <ReactiveFallback>
              <BallGrabbed color="blue"/>
              <Follow color="blueball"
                      id="{id}"/>
            </ReactiveFallback>
            <FollowPose frame_id="base_link"
                        goal="0;0;0"/>
            <SetIntakeMechanism dribble="-5.0"
                                lift="1.0"/>
            <NavToPose frame_id="base_link"
                       goal="0;0;0"/>
          </Sequence>
        </RetryUntilSuccessful>
        <Timeout msec="3000">
          <GetBallGrabbedTop color="blue"/>
        </Timeout>
        <SetIntakeMechanism dribble="0.0"
                            lift="0.0"/>
        <GetIntakeProximityArray pattern="000"/>
        <Sleep msec="500"/>
        <SetTailPosition angle="0.0"/>
      </Sequence>
    </RetryUntilSuccessful>
  </BehaviorTree>

  <BehaviorTree ID="optimizedfindball">
    <Sequence>
      <SetBlackboard value="NULL"
                     output_key="ball_state"/>
      <BallGrabbed color="{ball_state}"/>
      <SubTree ID="rotateToTarget"
               _autoremap="true"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="outtake">
    <Sequence>
      <SetIntakeMechanism dribble="0"
                          lift="0.8"/>
      <GetIntakeProximityArray pattern="001"/>
      <GetIntakeProximityArray pattern="000"/>
      <SetIntakeMechanism dribble="0"
                          lift="0"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="reactivetest">
    <ReactiveFallback>
      <GetBallGrabbed color_output=""
                      color_input="blue"/>
      <AMCLUpdate/>
    </ReactiveFallback>
  </BehaviorTree>

  <BehaviorTree ID="red_coba_nasional">
    <Sequence>
      <WaitButton/>
      <Repeat num_cycles="-1">
        <Sequence>
          <SubTree ID="regional_find&amp;follow"/>
          <SubTree ID="silo_testing"/>
          <SubTree ID="outtake"/>
        </Sequence>
      </Repeat>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="red_nasional">
    <Sequence>
      <WaitButton/>
      <SubTree ID="movemirror"
               _autoremap="false"/>
      <Sequence>
        <Repeat num_cycles="3">
          <Sequence>
            <SubTree ID="regional_find&amp;follow"/>
            <SubTree ID="red_silo2"/>
            <SubTree ID="outtake"/>
          </Sequence>
        </Repeat>
        <Repeat num_cycles="3">
          <Sequence>
            <SubTree ID="regional_find&amp;follow"/>
            <SubTree ID="red_silo3"/>
            <SubTree ID="outtake"/>
          </Sequence>
        </Repeat>
        <Repeat num_cycles="3">
          <Sequence>
            <SubTree ID="regional_find&amp;follow"/>
            <SubTree ID="red_silo4"
                     _autoremap="false"/>
            <SubTree ID="outtake"/>
          </Sequence>
        </Repeat>
      </Sequence>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="red_silo1">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="7.9;-4.7;1.57"/>
      <SetSpeed x="-0.8"
                y="0.0"
                theta="0.0"
                timeout="1000"/>
      <MoveWithLidarReference parent_frame="map"
                              child_frame="base_link"
                              x="0.0"
                              y="3.0"
                              speed="0.4"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="red_silo2">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="8.7;-4.7;1.57"/>
      <SetSpeed x="-0.8"
                y="0.0"
                theta="0.0"
                timeout="1000"/>
      <MoveWithLidarReference parent_frame="map"
                              child_frame="base_link"
                              x="0.0"
                              y="2.78"
                              speed="0.4"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="red_silo3">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="9.5;-4.7;1.57"/>
      <MoveWithLidarReference parent_frame="map"
                              child_frame="base_link"
                              x="0.0"
                              y="2.02"
                              speed="0.4"/>
      <SetSpeed x="-0.8"
                y="0.0"
                theta="0.0"
                timeout="1000"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="red_silo4">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="10.25;-4.7;1.57"/>
      <MoveWithLidarReference parent_frame="map"
                              child_frame="base_link"
                              x="0.0"
                              y="1.25"
                              speed="0.4"/>
      <SetSpeed x="-0.8"
                y="0.0"
                theta="0.0"
                timeout="1000"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="red_silo5">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="11.0;-4.7;1.57"/>
      <MoveWithLidarReference parent_frame="map"
                              child_frame="base_link"
                              x="0.0"
                              y="0.6"
                              speed="0.4"/>
      <SetSpeed x="-0.8"
                y="0.0"
                theta="0.0"
                timeout="1000"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="regional">
    <Sequence>
      <SubTree ID="movenew"
               _autoremap="true"/>
      <Sequence>
        <Repeat num_cycles="{silo4}">
          <Sequence>
            <SubTree ID="regional_find&amp;follow"
                     _autoremap="false"/>
            <SubTree ID="silo4"
                     _autoremap="false"/>
            <SubTree ID="outtake"/>
          </Sequence>
        </Repeat>
        <Repeat num_cycles="{silo3}">
          <Sequence>
            <SubTree ID="regional_find&amp;follow"/>
            <SubTree ID="silo3"/>
            <SubTree ID="outtake"/>
          </Sequence>
        </Repeat>
        <Repeat num_cycles="{silo2}">
          <Sequence>
            <SubTree ID="regional_find&amp;follow"/>
            <SubTree ID="blue_silo2"
                     _autoremap="false"/>
            <SubTree ID="outtake"
                     _autoremap="false"/>
          </Sequence>
        </Repeat>
      </Sequence>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="regional_find&amp;follow">
    <Parallel failure_count="1"
              success_count="-1">
      <Sequence>
        <SetIntakeMechanism dribble="5.0"
                            lift="1.0"/>
        <Sleep msec="1500"/>
        <SetTailPosition angle="0.7"/>
      </Sequence>
      <SubTree ID="oldfindnewball"
               _autoremap="false"/>
    </Parallel>
  </BehaviorTree>

  <BehaviorTree ID="regionaltest">
    <Repeat num_cycles="-1">
      <Sequence>
        <SubTree ID="regional_find&amp;follow"/>
        <SubTree ID="silo4"/>
        <SubTree ID="outtake"/>
      </Sequence>
    </Repeat>
  </BehaviorTree>

  <BehaviorTree ID="rotateToTarget">
    <Sequence>
      <GetIntakeProximityArray pattern="110"/>
      <SetIntakeMechanism dribble="0"
                          lift="0"/>
      <GetCurrentPose parent_frame_id="map"
                      child_frame_id="base_link"
                      pose="{outpose}"
                      x="{prev_x}"
                      y="{prev_y}"
                      theta="{prev_theta}"/>
      <RotateSpeed speed="6.0"
                   angle="0.0"
                   _skipIf="prev_x &gt;= 9.5"/>
      <RotateSpeed speed="6.0"
                   angle="-3.14"
                   _skipIf="prev_x &lt; 9.5"/>
      <SetIntakeMechanism dribble="-2.0"
                          lift="-2.0"/>
      <GetIntakeProximityArray pattern="000"/>
      <Sleep msec="1000"/>
      <RotateSpeed speed="6.0"
                   angle="{prev_theta}"/>
      <SetIntakeMechanism dribble="3"
                          lift="0.5"/>
      <Sleep msec="1000"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="setspeedtest">
    <Sequence>
      <NavToPose frame_id="base_link"
                 goal="0;0;0"/>
      <SetSpeed x="-2.0"
                y="0.0"
                theta="0.0"
                timeout="1000"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="setuptest">
    <Sequence>
      <SetupMenu silo2="{silo2}"
                 silo3="{silo3}"
                 silo4="{silo4}"/>
      <WaitButton/>
      <Repeat num_cycles="{silo2}">
        <SubTree ID="tailtest"/>
      </Repeat>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="silo1">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="7.95;3.0;-1.57 /&#10;7.95;4.0;-1.57"/>
      <AMCLUpdate/>
      <NavToPose frame_id="map"
                 goal="7.95;4.8;-1.57"/>
      <AMCLUpdate/>
      <FollowPose frame_id="map"
                  goal="7.95;5.1;-1.57"/>
      <Sleep msec="1000"/>
      <NavToPose frame_id="base_link"
                 goal="0;0;0"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="silo3">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="9.5;2.0;-1.57 /&#10;9.5;4.8;-1.57"/>
      <AMCLUpdate/>
      <NavToPose frame_id="map"
                 goal="9.5;4.8;-1.57"/>
      <SetSpeed x="-1.5"
                y="0.0"
                theta="0.0"
                timeout="1000"/>
      <MoveWithLidarReference parent_frame="map"
                              child_frame="base_link"
                              x="0.0"
                              y="2.02"
                              speed="1.5"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="silo4">
    <Sequence>
      <NavThroughPoses frame_id="map"
                       goals="9.5;2.0;-1.57 /&#10;10.3;4.8;-1.57"/>
      <AMCLUpdate/>
      <NavToPose frame_id="map"
                 goal="10.3;4.8;-1.57"/>
      <SetSpeed x="-1.5"
                y="0.0"
                theta="0.0"
                timeout="1000"/>
      <MoveWithLidarReference parent_frame="map"
                              child_frame="base_link"
                              x="0.0"
                              y="1.25"
                              speed="1.5"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="silo_testing">
    <Sequence>
      <NavToPose frame_id="map"
                 goal="9.5;-2.5;1.57"/>
      <GetBestSilo team_color="blueball"
                   mode="1"
                   silo="{silo_out}"/>
      <SubTree ID="red_silo1"
               _skipIf="silo_out != 5"/>
      <SubTree ID="red_silo2"
               _skipIf="silo_out != 4"
               _autoremap="false"/>
      <SubTree ID="red_silo3"
               _skipIf="silo_out != 3"/>
      <SubTree ID="red_silo4"
               _skipIf="silo_out != 2"/>
      <SubTree ID="red_silo5"
               _skipIf="silo_out != 1"/>
    </Sequence>
  </BehaviorTree>

  <BehaviorTree ID="silotest">
    <Repeat num_cycles="5">
      <Sequence>
        <NavThroughPoses frame_id="map"
                         goals="9.5;0.0;-1.57"/>
        <SubTree ID="blue_silo2"/>
        <NavThroughPoses frame_id="map"
                         goals="9.5;0.0;-1.57"/>
        <SubTree ID="silo3"/>
        <NavThroughPoses frame_id="map"
                         goals="9.5;0.0;-1.57"/>
        <SubTree ID="silo4"/>
      </Sequence>
    </Repeat>
  </BehaviorTree>

  <BehaviorTree ID="tabrak">
    <NavThroughPoses frame_id="map"
                     goals="8.5;1.5;-1.57 /&#10;9.5;0.5;-1.57"/>
  </BehaviorTree>

  <BehaviorTree ID="tailtest">
    <Repeat num_cycles="1">
      <Sequence>
        <SetIntakeMechanism dribble="3"
                            lift="1"/>
        <SetTailPosition angle="0.0"/>
        <Sleep msec="2000"/>
        <SetTailPosition angle="0.7"/>
        <Sleep msec="2000"/>
        <SetIntakeMechanism dribble="0"
                            lift="0"/>
      </Sequence>
    </Repeat>
  </BehaviorTree>

